{
  "dfx": "0.26.0",
  "canisters": {
    "user_canister": {
      "type": "rust",
      "package": "user_canister",
      "candid": "./.artifact/user_canister.did",
      "declarations": {
        "output": "./frontend/declarations/user_canister",
        "node_compatibility": true
      }
    },
    "frontend": {
      "build": "pnpm --filter frontend run build",
      "source": [
        "frontend/build/"
      ],
      "type": "assets",
      "declarations": {
        "output": "./frontend/declarations/frontend"
      }
    },
    "marketing": {
      "build": "pnpm --filter landing-page run build",
      "source": [
        "landing-page/dist/"
      ],
      "type": "assets",
      "declarations": {
        "output": "./frontend/declarations/marketing"
      }
    },
    "orchestrator": {
      "type": "rust",
      "package": "orchestrator",
      "candid": "./.artifact/orchestrator.did",
      "declarations": {
        "output": "./frontend/declarations/orchestrator"
      }
    },
    "orbit-station": {},
    "orbit-upgrader": {}
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8080",
      "type": "ephemeral",
      "replica": {
        "subnet_type": "system",
        "port": 4943
      }
    }
  },
  "version": 1
}