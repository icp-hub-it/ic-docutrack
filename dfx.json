{
  "dfx": "0.17.0",
  "canisters": {
    "user_canister": {
      "type": "rust",
      "package": "user_canister",
      "candid": "./.artifact/user_canister.did",
      "declarations": {
        "output": "declarations/user_canister",
        "node_compatibility": true
      }
    },
    "frontend": {
      "build": "pnpm --filter frontend run build",
      "dependencies": ["user_canister", "orchestrator"],
      "source": ["frontend/build/"],
      "type": "assets",
      "declarations": {
        "output": "declarations/frontend"
      }
    },
    "marketing": {
      "build": "pnpm --filter landing-page run build",
      "dependencies": ["frontend"],
      "source": ["landing-page/dist/"],
      "type": "assets",
      "declarations": {
        "output": "declarations/marketing"
      }
    },
    "orchestrator": {
      "type": "rust",
      "package": "orchestrator",
      "candid": "./.artifact/orchestrator.did",
      "declarations": {
        "output": "declarations/orchestrator"
      }
    },
    "internet_identity": {
      "type": "pull",
      "id": "rdmx6-jaaaa-aaaaa-aaadq-cai",
      "declarations": {
        "output": "declarations/internet_identity"
      }
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
